{
    "name": "fei/connect-package",
    "description": "Connect client package integration for Objective PHP applications",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Yoctu",
            "email": "contact@yoctu.com",
            "homepage": "http://yoctu.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fei\\Service\\Connect\\Package\\": "src/"
        }
    },
    "repositories" : [
        {
            "type" : "git",
            "url": "git@github.com:flash-global/connect-client.git"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0.27"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-openssl": "*",
        "objective-php/application": "^2.0",
        "fei/connect-client": "~3.0.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.4",
        "squizlabs/php_codesniffer": "^3"
    }
}
